        ],
        "description": "class method"
    },
    "initmethod": {
        "prefix": ["init"],
        "body": [
            "def __init__(self, ${1:arg}):",
            "    ${2:pass}"
        ],
        "description": "class init method"
    },
    "file_env": {
        "prefix": ["fh"],
        "body": [
            "#!/usr/bin/env python",
            "# -*- coding:utf-8 -*-",
            "# Author: $1",
            "# Description: $2"
        ],
        "description": "file env head"
    },
    "for_enumerate": {
        "prefix": ["fore"],
        "body": [
            "for ${1:i}, ${2:value} in enumerate($3):",
            "    ${4:pass}"
        ],
        "description": "for_enumerate"
    },
    "for_range": {
        "prefix": ["forr"],
        "body": [
            "for ${1:i} in range($2):",
            "    ${3:pass}"
        ],
        "description": "for_range"
    }
